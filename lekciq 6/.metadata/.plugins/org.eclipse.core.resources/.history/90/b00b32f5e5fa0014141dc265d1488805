import java.util.Scanner;


public class Cats {

	public static void main(String[] args) {
     Scanner input=new Scanner(System.in,"UTF-8");
     String name;
     int years;
     Cat[] mass=new Cat[10];
     for(int i=0;i<2;i++){
    	 
    	 System.out.println("Enter name for cat:");
    	 name=input.nextLine();
    	 System.out.println("Enter name for cat:");
    	 years=input.nextInt();
    	 mass[i]=new Cat(name,years);
     }
     for(int k=0;k<10;k++){
    	 mass[k].Say();
     }

	}

}
